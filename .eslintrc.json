{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/recommended",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "indent": ["error", "tab"],
        "linebreak-style": ["error", "windows"],
        "no-tabs": 0,
        "prefer-destructuring": ["error", {"object": true, "array": false}],
        "no-console": "off"
    }
}
